{
  "version": 2,
  "buildCommand": "cd adapter-lover && npm run build",
  "outputDirectory": "adapter-lover/build",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}